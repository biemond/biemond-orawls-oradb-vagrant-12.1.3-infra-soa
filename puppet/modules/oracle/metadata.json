{
  "name": "hajee-oracle",
  "version": "0.7.0",
  "author": "Bert Hajee",
  "summary": "the custom types needed to manage the basic setup of an Oracle database",
  "license": "Apache-2.0",
  "source": "https://github.com/hajee/oracle",
  "project_page": "https://github.com/hajee/oracle",
