
wlsUser    = '<%= @weblogic_user %>'
password   = sys.argv[1] 
machine    = '<%= @adminserver_address %>'
portNumber = '<%= @adminserver_port %>'

opss_datasource = '<%= @repository_prefix %>_OPSS'
domain_name     = '<%= @domain_name %>'

connect(wlsUser,password,'t3://'+machine+':'+portNumber)

reassociateSecurityStore(domain=domain_name, servertype="DB_ORACLE",datasourcename="jdbc/opssDataSource", jpsroot="cn=SecurityStore", join="false")