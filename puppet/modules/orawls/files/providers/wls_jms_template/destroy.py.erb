# check the domain else we need to skip this (done in wls_access.rb)
real_domain='<%= domain %>'



name               = '<%= template_name %>'
jmsmodule          = '<%= jmsmodule %>'

edit()
startEdit()

try:

    cd('/JMSSystemResources/'+jmsmodule+'/JMSResource/'+jmsmodule)
    cmo.destroyTemplate(getMBean('/JMSSystemResources/'+jmsmodule+'/JMSResource/'+jmsmodule+'/Templates/'+name))
    save()
    activate()
    print "~~~~COMMAND SUCCESFULL~~~~"

except:
    print "Unexpected error:", sys.exc_info()[0]
    undo('true','y')
    stopEdit('y')
    print "~~~~COMMAND FAILED~~~~"
    raise
