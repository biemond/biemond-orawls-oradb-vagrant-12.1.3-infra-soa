m = ls('/CoherenceServers',returnMap='true')

f = open("/tmp/wlstScript.out", "w")
print >>f, "name;autorestart;machine;restartdelay;restartinterval;restartmax;unicastaddress;unicastport;domain"
for token in m:
    print '___'+token+'___'
    cd('/CoherenceServers/'+token)

    n = ls('/CoherenceServers/'+token+'/Machine')
    machine = 'none'
    for token2 in n.split("drw-"):
        token2=token2.strip().lstrip().rstrip()
        if token2:
             machine = token2

    print >>f, ";".join(map(quote, [domain+'/'+token,get('AutoRestart'),machine,get('RestartDelaySeconds'),get('RestartIntervalSeconds'),get('RestartMax'),get('UnicastListenAddress'),get('UnicastListenPort'),domain]))
f.close()
